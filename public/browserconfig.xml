<!-- Custom tiles config for Microsoft Windows OS and Microsoft Edge browser -->
<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
  <msapplication>
    <tile>
      <square70x70logo src="images/favicon-70x70.png"/>
      <square150x150logo src="images/favicon-150x150.png"/>
      <wide310x150logo src="images/favicon-310x150.png"/>
      <square310x310logo src="images/favicon-310x310.png"/>
      <TileColor>#222222</TileColor>
    </tile>
  </msapplication>
</browserconfig>