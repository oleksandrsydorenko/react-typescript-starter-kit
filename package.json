    "lint-staged": "^12.3.3",
    "loader-utils": "^3.2.0",
    "mini-css-extract-plugin": "^2.5.3",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^25.0.0",
