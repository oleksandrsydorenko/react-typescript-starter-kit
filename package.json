{
  "name": "react-happy-kit",
  "version": "1.0.0",
  "description": "React-Typescript Starter Kit",
  "author": "Oleksandr Sydorenko (https://www.linkedin.com/in/oleksandrsydorenko)",
  "license": "MIT",
  "homepage": "https://github.com/oleksandrsydorenko/react-typescript-starter-kit#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/oleksandrsydorenko/react-typescript-starter-kit.git"
  },
  "bugs": {
    "url": "https://github.com/oleksandrsydorenko/react-typescript-starter-kit/issues"
  },
  "scripts": {
    "build": "yarn clear:cache && cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.config.js",
    "build:stats": "cross-env STATS_PRESET=verbose yarn build --progress --profile",
    "clear": "rimraf ./build ./node_modules",
    "clear:cache": "rimraf ./node_modules/.cache",
    "lint": "yarn lint:eslint & yarn lint:prettier & yarn lint:stylelint",
    "lint:eslint": "eslint --fix \"**/*.{j,t}s{,x}\"",
    "lint:prettier": "prettier -w \"**/*.{html,json,md,yaml}\"",
    "lint:stylelint": "stylelint --fix \"src/**/*.{c,sa,sc}ss\"",
    "postinstall": "yarn clear:cache",
    "start": "yarn clear:cache && cross-env NODE_ENV=development nodemon -x \"webpack serve --config ./config/webpack/webpack.config.js\"",
    "test:coverage": "jest --coverage",
    "test:debug": "jest --inspect-brk --runInBand",
    "test:silent": "jest --silent --passWithNoTests",
    "test:watch": "jest --watch",
    "ts:build": "rimraf ./declarations/generated && tsc",
    "ts:check": "yarn tsc --noEmit",
    "ts:watch": "yarn ts:check --watch"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-transform-react-constant-elements": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt-cli": "^11.0.0",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@svgr/webpack": "^5.4.0",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
