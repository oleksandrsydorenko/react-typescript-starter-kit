{
  "name": "react-typescript-starter-kit",
  "version": "1.0.0",
  "description": "React-TypeScript Starter Kit",
  "main": "./src/index.tsx",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.config.js",
    "build:stats": "cross-env WEBPACK_STATS=verbose yarn build --progress --profile",
    "clear:cache": "rimraf node_modules/.cache",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:stylelint",
    "lint:eslint": "eslint --fix \"**/*.{j,t}s{,x}\"",
    "lint:prettier": "prettier -w \"**/*.{html,json,md,yaml}\"",
    "lint:stylelint": "stylelint --fix \"src/**/*.{c,sa,sc}ss\"",
    "postinstall": "yarn clear:cache",
    "start": "nodemon",
    "test:coverage": "jest --coverage",
    "test:debug": "jest --inspect-brk --runInBand",
    "test:silent": "jest --silent --passWithNoTests",
    "test:watch": "jest --watch",
    "ts:check": "yarn tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oleksandrsydorenko/react-typescript-starter-kit.git"
  },
  "author": "Oleksandr Sydorenko (https://www.linkedin.com/in/oleksandrsydorenko)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oleksandrsydorenko/react-typescript-starter-kit/issues"
  },
  "homepage": "https://github.com/oleksandrsydorenko/react-typescript-starter-kit#readme",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-react-constant-elements": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.5.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "cache-loader": "^4.1.0",
    "compression-webpack-plugin": "^9.2.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fork-ts-checker-webpack-plugin": "^7.1.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
